                    <span>3 Products</span>
                </h4>
                <table class="timetable_sub">
                    <thead>
                        <tr>
                            <th>SL No.</th>
                            <th>Product</th>
                            <!-- <th>Quantity</th> -->
                            <th>Product Name</th>

                            <!-- <th>Price</th> -->
                            <th>Remove</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% user.forEach(function(user , index) { %>
                        <tr class="rem1">

                            <td class="invert"><%= index+1 %></td>
                            <td class="invert-image">
                                <a href="single.html">
                                    <img src="./image/<%= user._id %>.jpeg" alt=" " class="img-responsive">
                                </a>
                            </td>
                            <td class="invert"><%=user.name %></td>
                            <!-- <td class="invert">
                                <div class="quantity">
                                    <div class="quantity-select">
                                        <a href="/removeFromcart/<%=user._id%>"><div class="entry value-minus">&nbsp;</div></a>
                                        <div class="entry value"><span><%=user.count%></span></div>
                                        <a href="/addtoCart/<%=user._id%>"><div class="entry value-plus active">&nbsp;</div></a>
                                    </div>
                                </div>
                            </td> -->
                            

                            <!-- <td class="invert">&#x20B9; <%= (user.price * user.count) %></td> -->
                            <td class="invert">
                                <div class="rem">
                                    <i class='fa fa-trash-o'></i>
                                     <!-- <div class="close1"> </div> -->
                                </div>
                            </td>	
                        </tr>
                        <% }) %> 
                    </tbody>
                </table>
            </div>
        </div>

    </div>
</section>